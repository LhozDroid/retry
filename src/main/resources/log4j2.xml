<?xml version="1.0" encoding="UTF-8"?>
<?xml-model href="http://logging.apache.org/log4j/2.0/config"?>
<Configuration status="info">
	<Appenders>
		<Console name="ConsoleAppender" target="SYSTEM_OUT">
			<PatternLayout pattern="%d{yyyy-MM-dd HH:mm:ss.SSS} %-5.5t %-5.5p - [%-30.30C:%5L] - %m%n" />
		</Console>
	</Appenders>
	<Loggers>
		<Root level="debug">
			<AppenderRef ref="ConsoleAppender" />
		</Root>
	</Loggers>
</Configuration>